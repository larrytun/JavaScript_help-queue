<div id="new-ticket-form" class="col-md-6 col-md-offset-3 col-sm-8 col-sm-offset-2 text-center">
  <form>
    
    <div class="form-group">
        <label for="title">Please enter your name</label>
        {{input value=name id="name"}}
      </div>
      <div class="form-group">
        <label for="location">Please enter your location</label>
        {{input value=location id="location"}}
      </div>
      <div class="form-group">
        <label for="description">Give a brief description of issue you are having</label>
        {{input value=description id="description"}}
      </div>
      <button class="btn btn-danger"{{action 'saveQueue1'}}>{{#link-to 'help'}}Help!{{/link-to}}</button>

  </form>
</div>
